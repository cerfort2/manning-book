{
    "resource": [
        {
            "google_compute_instance": [
                {
                    "hello-world": [
                        {
                            "allow_stopping_for_update": true,
                            "boot_disk": [
                                {
                                    "initialize_params": [
                                        {
                                            "image": "ubuntu-1804-lts"
                                        }
                                    ]
                                }
                            ],
                            "machine_type": "f1-micro",
                            "name": "hello-world",
                            "network_interface": [
                                {
                                    "network_ip": "10.0.0.5",
                                    "subnetwork": "hello-world-subnet"
                                }
                            ],
                            "zone": "us-central1-a"
                        }
                    ]
                }
            ]
        }
    ]
}